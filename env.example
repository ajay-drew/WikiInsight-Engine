# Environment Variables Example
# Copy this file to .env and fill in your values

# API Keys (if needed)
# WIKIPEDIA_API_KEY=

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/wikiinsight
REDIS_URL=redis://localhost:6379/0

# MLflow
MLFLOW_TRACKING_URI=sqlite:///mlflow.db
MLFLOW_EXPERIMENT_NAME=wikiinsight

# DVC Remote Storage (optional)
DVC_REMOTE_URL=
DVC_REMOTE_TYPE=local

# Logging
LOG_LEVEL=INFO

# API Settings
API_HOST=0.0.0.0
API_PORT=8000

# Dashboard
DASHBOARD_PORT=8505

